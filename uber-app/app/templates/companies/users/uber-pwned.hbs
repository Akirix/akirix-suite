<div class="viewport viewport-t-pane-secondary viewport-t-pane-secondary-with-toolbar scroll-y">
    <div class="pane-secondary-content">
        <div id="uber-pwned" class="row">
            <div class="col-md-12">
                <fieldset>
                    <div class="panel panel-default">
                        <div class="panel-heading text-center">Pwned History</div>
                        <table class="table table-hover">
                            {{#if model}}
                                <thead>
                                <tr>
                                    <th class="col-md-4">Name</th>
                                    <th class="col-md-4">Date</th>
                                    <th class="col-md-4">Type</th>

                                </tr>
                                </thead>
                                <tbody>
                                {{#each uber_pwned in model}}
                                    {{#link-to 'companies.view.users.view.uber-pwned.pwned' uber_pwned.id tagName='tr' class='list-link'}}

                                        <td class="valign-middle small">{{uber_pwned.name}}</td>


                                        <td class="valign-middle small">{{uber_pwned.str_created_at_date}}</td>


                                        <td class="valign-middle small">{{uber-pwned-type uber_pwned.type}}</td>

                                    {{/link-to}}
                                {{/each}}
                                </tbody>
                            {{else}}
                                <tbody>
                                <tr>
                                    <td class="valign-middle text-center">
                                        <h5>No Pwneds Found</h5>
                                    </td>
                                </tr>
                                </tbody>
                            {{/if}}
                        </table>
                    </div>
                </fieldset>
            </div>
        </div>
    </div>
</div>
<div class="viewport viewport-bottom navbar-toolbar">
    <div class="pane-actions">
        <ul class="list-h">
            <li>
                <button {{action 'goBack'}} class="btn btn-default navbar-secondary-btn btn-sm" type="button">
                    Back
                </button>
            </li>
        </ul>
    </div>
</div>
