<table class="table table-hover table-high">
    <thead>
    <tr>
        <th class="col-xs-1">Date</th>
        <th class="col-xs-4">Filename</th>
        <th class="col-xs-4">Related To</th>
        <th class="col-xs-1">Admin User</th>
        <th class="col-xs-1"></th>
    </tr>
    </thead>
    <tbody>
    {{#each doc in model}}
        {{#link-to 'companies.view.documents.uber-documents.view' doc.id tagName='tr' class='list-link'}}
            <td class="valign-middle">
                <strong>{{doc.str_created_at_date}}</strong><br/>
                {{doc.str_created_at_time}}
            </td>
            <td class="valign-middle">
                {{file-icon doc.type}} {{doc.name}}
            </td>
            <td class="valign-middle">
                {{document-model-name document=doc}}
            </td>
            <td class="valign-middle">
                {{doc.uberUser.name}}
            </td>
            <td class="valign-middle">

            </td>
        {{/link-to}}
    {{/each}}
    </tbody>
</table>

