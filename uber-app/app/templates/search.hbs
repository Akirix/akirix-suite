<div class="viewport viewport-full viewport-content-with-nav">
    <div class="viewport-t-pane-primary viewport-t-pane-primary-with-toolbar scroll-y">
        <div class="pane-primary">
            <div class="navbar-secondary" role="navigation">
            </div>

            <div class="pane-primary-content">
                <div class="row">
                    <div class="col-xs-12 form-group">
                        <h2>Search: {{currentSearch}}</h2>
                    </div>
                    {{search-result itemName='companies' endpoint='/companies/search' template='components/search-result-companies' searchValue=currentSearch per_page=20 page =1}}
                    {{search-result itemName='users' endpoint='/users/search' template='components/search-result-users' searchValue=currentSearch per_page=20 page =1}}
                    {{search-result itemName='wires' endpoint='/wires/search' template='components/search-result-wires' searchValue=currentSearch per_page=20 page =1}}
                    {{search-result itemName='fx_requests' endpoint='/fxRequests/search' template='components/search-result-fx-requests' searchValue=currentSearch per_page=20 page =1}}
                    {{search-result itemName='registrations' endpoint='/signupRegistrations/search' template='components/search-result-registrations' searchValue=currentSearch per_page=20 page =1}}
                    {{search-result itemName='account_alias' endpoint='/accountAliases/search' template='components/search-result-account-aliases' searchValue=currentSearch per_page=20 page =1}}
                </div>
            </div>
        </div>
    </div>
    <div class="viewport-right pane pane-secondary">
        {{outlet 'paneSecondary'}}
    </div>
</div>
