<div class="navbar-secondary" role="navigation">
    <ul class="list-h">
    </ul>
</div>
<div class="pane-primary-content">
    {{partial 'nav-controls'}}

{{#if content}}
    {{page-numbers content=content}}
{{/if}}
<table class="table table-hover">
    <thead>
    <tr>
        <th class="col-xs-1">Date</th>
        <th class="col-xs-3">User</th>
        <th class="col-xs-3">Company</th>
        <th class="col-xs-3">Location</th>
        <th class="col-xs-2">Service Provider</th>
    </tr>
    </thead>
    <tbody>
    {{#each accessLog in model}}
        {{#link-to 'access-logs.index.view' accessLog.id tagName='tr' class='list-link'}}
            <td class="valign-middle small">
                <strong>{{accessLog.str_created_at_date}}</strong><br/>
                {{accessLog.str_created_at_time}}
            </td>
            <td class="valign-middle">
                {{accessLog.user.first_name}}  {{accessLog.user.last_name}}<br/>
                <span class="small" >{{accessLog.user.email}}</span>
            </td>
            <td class="valign-middle">
                {{accessLog.user.company.name}}<br/>
                <span class="small">XYZ{{accessLog.user.company.account_number}}</span>
            </td>
            <td class="valign-middle">
                {{accessLog.country}}<br/>
                <span class="small">{{accessLog.city}} {{accessLog.state}}</span>
            </td>
            <td class="valign-middle">
                {{accessLog.ip}}<br/>
                <span class="small">{{accessLog.service_provider}}</span>
            </td>
        {{/link-to}}
    {{/each}}
    </tbody>
</table>
{{#if content}}
    {{page-numbers content=content}}
{{/if}}
</div>
