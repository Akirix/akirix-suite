<div class="side-panel-content">
    <fieldset class="akx-fieldset">
        <legend align="center">{{currencyId}} {{stringList.pendingApproval}}</legend>
    </fieldset>
    <div class="row border-bottom py-4">
        <div class="col-lg-6 text-right">
            <strong>{{stringList.submitted}}:</strong>
        </div>
        <div class="col-lg-6">
            {{format-date model.ticket.created_at}}
        </div>
    </div>
    <div class="row border-bottom py-4">
        <div class="col-lg-6 text-right">
            <strong>{{stringList.currencyType}}:</strong>
        </div>
        <div class="col-lg-6">
            {{currencyId}}
        </div>
    </div>
    <div class="row border-bottom py-4">
        <div class="col-lg-6 text-right">
            <strong>{{stringList.inFrequency}}:</strong>
        </div>
        <div class="col-lg-6">
            {{inVolumeFrequency}}
        </div>
    </div>
    <div class="row border-bottom py-4">
        <div class="col-lg-6 text-right">
            <strong>{{stringList.inVolume}}:</strong>
        </div>
        <div class="col-lg-6">
            {{inVolume}}
        </div>
    </div>
    <div class="row border-bottom py-4">
        <div class="col-lg-6 text-right">
            <strong>{{stringList.outFrequency}}:</strong>
        </div>
        <div class="col-lg-6">
            {{outVolumeFrequency}}
        </div>
    </div>
    <div class="row border-bottom py-4">
        <div class="col-lg-6 text-right">
            <strong>{{stringList.outVolume}}:</strong>
        </div>
        <div class="col-lg-6">
            {{outVolume}}
        </div>
    </div>
    <div class="row border-bottom py-4">
        <div class="col-lg-6 text-right">
            <strong>{{stringList.averageBalance}}:</strong>
        </div>
        <div class="col-lg-6">
            {{balance}}
        </div>
    </div>
    <div class="row mt-3">
        <div class="col-lg-12">
            {{stringList.helpParagraph}}
        </div>
    </div>
</div>